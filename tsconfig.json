{
  "extends": "./node_modules/gts/tsconfig-google.json",
  "compilerOptions": {
    "target": "es5",
    "rootDir": ".",
    "outDir": "dist",
    "lib": [
      "es5",
      "es6",
      "es7",
    ],
    "module": "esnext",
    "downlevelIteration": true,
    "strict": true,
    "noUnusedLocals": true,
    "esModuleInterop": true,
    "moduleResolution": "node"
  },
  "include": [
    "src/**/*.ts",
    "tests/**/*.ts"
  ]
}
